/**
 * Definition for singly-linked list.
 * class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution {
    public boolean hasCycle(ListNode head) {
        HashSet<ListNode> nodes=new HashSet<>();
        ListNode current=head;

        while(current!=null){
            if(nodes.contains(current)){
                return true;
            }
            nodes.add(current);
            current=current.next;
        }
        return false;
    }
}
