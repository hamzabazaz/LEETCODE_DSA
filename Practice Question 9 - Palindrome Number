class Solution {
    public boolean isPalindrome(int x) {
        int n=x;
        int res=0;
        if(n<0){
            return false;
        }
        while(n!=0){
            
            int digit=n%10;
            res=res*10+digit;
            n/=10;
        }
        if(res==x){
            return true;
        }
        return false;
    }
}
