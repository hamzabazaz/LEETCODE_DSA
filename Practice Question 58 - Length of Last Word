class Solution {
    public int lengthOfLastWord(String s) {
        int count=0;
        int j=s.length()-1;
        while(j>=0){
            if(s.charAt(j)!=' '){
                break;
            }else{
                j--;
            }
        }
        for(int i=j;i>=0;i--){
            if(s.charAt(i)!=' ') count++;
            else{
                 break;
            }
           
        }
        return count;
        
    }
}
