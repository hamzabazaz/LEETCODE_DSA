/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    public List<Integer> preorderTraversal(TreeNode root) {
        List<Integer>res=new ArrayList<>();
        pt(root,res);
        return res;
    }

    private List<Integer> pt(TreeNode root, List<Integer>res){
        if(root==null){
            return res;
        }
        res.add(root.val);
        pt(root.left,res);
        pt(root.right,res);

        return null;
    }
}
